{
    "mcpServers": {
        "gemini": {
            "command": "node",
            "args": [
                "/Users/kwak/Projects/ai/gemini-oauth-mcp/dist/index.js"
            ],
            "env": {
                "LOG_LEVEL": "info"
            },
            "defer_loading": true
        },
        "context7": {
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp@latest"
            ],
            "defer_loading": true
        },
        "playwright": {
            "command": "npx",
            "args": [
                "-y",
                "@anthropic-ai/mcp-server-playwright@latest"
            ],
            "defer_loading": true
        }
    }
}